		"noImplicitAny": false,
		"declaration": true,
		"removeComments": true,
		"outFile": "dist/tui2.js",
		"sourceMap": true,
		"noEmitOnError": true
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"depends",
		"dist",
		"doc",
		"extentions",
		"less",
		"node_modules",
		"lang",
		"test"
	]
}
